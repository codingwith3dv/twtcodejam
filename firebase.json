{
  "database": {
    "rules": "src/database.rules.json"
  },
  "hosting": {
    "site": "go-off-beat",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
